<!-- /templates/page.liquid -->
<div class="grid page">

  <div class="grid__item large--one-whole">

    <div class="page-title">
      <h1 class="brush-underline zero-padding text-center page-header-style">{{ page.title }}</h1>
    </div>

    <div class="grid flex-middle-align small--flex-align-none">
      <div class="grid__item push--large--one-twelfth large--five-twelfths small--margin-btm text-center">
        <img src="{{ 'licensing.png' | asset_url }}" alt="{{ shop.name }}">
      </div>
      {% comment %}
        Regular page content goes here.
      {% endcomment %}
      <div class="grid__item push--large--one-twelfth large--five-twelfths">
        <div class="rte">
          {{ page.content }}
        </div>
      </div>
    </div>

  </div>

</div>
